<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="privilege" extends="json-all">
		<action name="saveItem" class="com.cqtd.common.ui.taglib.action.ZTreeAction"
			method="saveItem">
		</action>
		<action name="editItem" class="com.cqtd.common.ui.taglib.action.ZTreeAction"
			method="editItem">
		</action>
		<action name="removeItem" class="com.cqtd.common.ui.taglib.action.ZTreeAction"
			method="removeItem">
		</action>
		<action name="west" class="com.cqtd.common.ui.taglib.action.ZTreeAction"
			method="west">
			<result>/page/west.jsp</result>
		</action>
		<action name="renderWestPanel" class="com.cqtd.common.ui.taglib.action.ZTreeAction"
			method="renderWestPanel">
		</action>
		<action name="menuTree" class="com.cqtd.common.ui.taglib.action.ZTreeAction"
			method="getMenuTree">
		</action>
		<action name="privilegeTree" class="com.cqtd.common.ui.taglib.action.ZTreeAction"
			method="privilegeTree">
		</action>
		<action name="privilege" class="com.cqtd.common.ui.taglib.action.ZTreeAction"
			method="privilege">
			<result>/page/privilege.jsp</result>
		</action>
		<action name="initSystemMenuTree" class="com.cqtd.common.ui.taglib.action.ZTreeAction"
			method="initSystemMenuTree">
		</action>
	</package>
</struts>